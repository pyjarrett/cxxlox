fun helloFunctions() {
    print "Hello, World!";
}

fun sum(a, b) {
    return a + b;
}

fun five() {
    return 5;
}

fun fib(n) {
    if (n < 2) return n;
    return fib(n - 1) + fib(n - 2);
}

helloFunctions();
print five();
print sum(1,2);

for (var i = 0; i < 10; i = i + 1) {
    print fib(i);
}
